%import 
        showRegistrationForm,
        showLoginForm,
        showDashboard,
        showJobs,
        login,
        logout,
        createEmployer,
        createJob,
        showProfile 
from "./handlers"

GET / showJobs

GET /register showRegistrationForm

POST /register  createEmployer

GET /login showLoginForm

POST /login login

GET /logout logout

GET /dashboard showDashboard

POST /api/jobs createJob

GET /jobs/:id showProfile
